                                                        <![CDATA[from random import choice
from time import sleep

def con_killer(step):
    m=max([count_cons(x) for x in step])
    return filter(lambda x: count_cons(x)==m, step)

def count_vowels(word):
    counter=0
    vowels=['a', 'e', 'i', 'o', 'u', 'y']
    for letter in word:
        if letter in vowels:
            counter+=1
    return counter
count_cons=lambda x: 5-count_vowels(x)

def pop_count(word):
    freq_dict={
        'a': 0.08167,
        'b': 0.01492,
        'c': 0.02782,
        'd': 0.04253,
        'e': 0.130001,
        'f': 0.02228,
        'g': 0.02015,
        'h': 0.06094,
        'i': 0.06966,
        'j': 0.00153,
        'k': 0.00772,
        'l': 0.04025,
        'm': 0.02406,
        'n': 0.06749,
        'o': 0.07507,
        'p': 0.01929,
        'q': 0.00095,
        'r': 0.05987,
        's': 0.06327,
        't': 0.09056,
        'u': 0.02758,
        'v': 0.00978,
        'w': 0.02360,
        'x': 0.00150,
        'y': 0.01974,
        'z': 0.00074
        }
    pop=0
    for x in word:
        pop+=freq_dict[x]
    return pop

def freq_killer(step):
    m=max([pop_count(x) for x in step])
    return filter(lambda x:pop_count(x)==m, step)

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self)
        pass

    def onLoad(self):
        self.talkProxy = ALProxy("ALTextToSpeech")
        self.memoryProxy = ALProxy("ALMemory")
        #~ puts code for box initialization here
        pass

    def onUnload(self):
        #~ puts code for box cleanup here
        pass

    def onInput_onStart(self):
        step = self.memoryProxy.getData("step")
        attempts = self.memoryProxy.getData("attempts")
        if attempts<2:
            step = con_killer(step)
            #self.talkProxy.say("Lets try a word with a lot of consonants!")
        elif attempts==4:
            step = freq_killer(step)
            self.talkProxy.say("Now, Let's try a common word!")
        else:
            self.talkProxy.say("I'm fishing in the dark here!")
        guess = choice(step)
        self.talkProxy.say("Is it %s?" % guess)
        for n in guess:
            self.talkProxy.say(n)
            sleep(.25)
        self.memoryProxy.insertData("attempts", attempts + 1)
        self.memoryProxy.insertData("guess", guess)
        self.onStopped()

    def onInput_onStop(self):
        self.onUnload() #~ it is recommended to call onUnload of this box in a onStop method, as the code written in onUnload is used to stop the box as well
        pass]]>
