        #~ puts code for box initialization here
        pass

    def onUnload(self):
        #~ puts code for box cleanup here
        pass

    def onInput_onStart(self):
        self.memoryProxy.insertData("words", words)
        self.memoryProxy.insertData("step", [x for x in words])
        self.memoryProxy.insertData("guess", "")
        self.memoryProxy.insertData("score", "00")
        self.memoryProxy.insertData("attempts", 0)
        self.memoryProxy.insertData("error_attempts", 0)
        self.memoryProxy.insertData("aux_step", [x for x in words])
        self.memoryProxy.insertData("moves", list())
        self.onStopped()

    def onInput_onStop(self):
        self.onUnload() #~ it is recommended to call onUnload of this box in a onStop method, as the code written in onUnload is used to stop the box as well
        pass]]>
