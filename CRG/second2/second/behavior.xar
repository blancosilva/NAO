        error_attempts = self.memoryProxy.getData("error_attempts")
        aux_step = self.memoryProxy.getData("aux_step")
        moves = self.memoryProxy.getData("moves")
        word = moves[error_attempts][:5]
        score = moves[error_attempts][5:]
        aux_step = match(aux_step, word, score)

        # after
        self.memoryProxy.insertData("error_attempts", error_attempts+1)
        self.memoryProxy.insertData("aux_step", aux_step)
        self.onStopped() #~ activate output of the box
        pass

