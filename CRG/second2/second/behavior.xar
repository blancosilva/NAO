                                    <![CDATA[def compute_score(word, guess):
    correct=0
    moved=0
    for x in range(5):
        if guess[x] in word:
            if guess[x]==word[x]:
                correct+=1
            else:
                moved+=1
    return str(correct)+str(moved)

def match(step, guess, score):
    return filter(lambda x: compute_score(x, guess)== score, step)

