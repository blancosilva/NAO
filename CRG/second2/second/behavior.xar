                                    <![CDATA[from urllib2 import urlopen

def no_repeats(word):
    new_word = ""
    for letter in word:
        if letter not in new_word:
            new_word += letter
    return new_word == word

file = urlopen("http://www.math.sc.edu/~blanco/words.txt")
words = file.read().split("\n")
file.close()

words = filter(lambda x: len(x)==5, words)
words = filter(no_repeats, words)
print words

